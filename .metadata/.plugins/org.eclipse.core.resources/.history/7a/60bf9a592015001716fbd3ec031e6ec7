package hello;

import java.util.Scanner;

public class Hello {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
//		System.out.println("Hello World中文");
		Scanner in = new Scanner(System.in);
//		System.out.println("ECHO:" + in.nextLine());
		int price = in.nextInt();
