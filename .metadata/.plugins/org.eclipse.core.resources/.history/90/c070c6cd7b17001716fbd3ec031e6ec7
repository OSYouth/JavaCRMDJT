import java.util.Scanner;

public class forloop {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int factor = 1;
		int i = 1;
		while(i <= n)
		{
			factor = factor * i;
			i = i + 1 ;
		}
//		for(int i = 1; i <= n; i++)
//		{
//			factor = factor * i;
//		}
		System.out.println(factor);
	}

}
